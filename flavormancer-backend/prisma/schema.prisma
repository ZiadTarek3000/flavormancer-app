generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Category {
  id    Int     @id @default(autoincrement())
  img   String
  title String
  desc  String
  price Float
}

model SpecialMenuCard {
  id        Int      @id @default(autoincrement())
  img       String
  title     String
  price     Float
  rating    Int      @default(5)
  reviews   Int      @default(169)
}

model RegularFoodCard {
  id        Int      @id @default(autoincrement())
  img       String
  title     String
  desc      String
  price     Float
  rating    Int      @default(5)
  reviews   Int      @default(169)
}

model Chef {
  id     Int     @id @default(autoincrement())
  img    String
  offset Boolean @default(false)
}


